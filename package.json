{
    "name": "@jsdocs-io/extractor",
    "description": "Analyze and extract the API from npm packages",
    "version": "0.4.0",
    "author": "Edoardo Scibona (velut)",
    "license": "AGPL-3.0-or-later",
    "source": "./src/index.ts",
    "main": "./dist/index.js",
    "module": "./dist/extractor.esm.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/extractor.esm.js",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "src",
        "dist"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "dts watch --target node",
        "build:check": "tsc --noEmit",
        "build": "tsc --noEmit && dts build --target node",
        "test": "dts test --runInBand --testTimeout=30000",
        "test:packages": "dts test test/package-snapshots --maxWorkers=4 --testTimeout=30000",
        "test:cov": "dts test --coverage --maxWorkers=4 --testTimeout=30000",
        "test:watch": "dts test --runInBand --watchAll --coverage",
        "test:ci": "dts test --coverage --maxWorkers=2 --testTimeout=30000 --bail --ci",
        "lint": "dts lint src test",
        "lint:fix": "dts lint src test --fix",
        "debug:analyze": "pnpm tsx scripts/debug-analyze.ts",
        "prepare": "husky install && pnpm build",
        "pre-push": "pnpm i && pnpm lint && pnpm build && pnpm test:cov",
        "release": "np"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jsdocs-io/extractor.git"
    },
    "bugs": {
        "url": "https://github.com/jsdocs-io/extractor/issues"
    },
    "keywords": [
        "npm",
        "package",
        "analyzer",
        "analyze",
        "info",
        "api",
        "extractor",
        "typescript",
        "javascript",
        "jsdocs",
        "jsdocs.io"
    ],
    "dependencies": {
        "@microsoft/tsdoc": "^0.14.2",
        "concat-stream": "^2.0.0",
        "debug": "^4.3.4",
        "got": "^11.8.6",
        "gunzip-maybe": "^1.4.2",
        "hosted-git-info": "^7.0.0",
        "prettier": "^2.8.8",
        "query-registry": "^2.6.0",
        "tar-stream": "^3.1.6",
        "ts-morph": "^20.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.8.0",
        "@commitlint/config-conventional": "17.7.0",
        "@pollyjs/adapter-node-http": "6.0.6",
        "@pollyjs/core": "6.0.6",
        "@pollyjs/persister-fs": "6.0.6",
        "@types/concat-stream": "2.0.0",
        "@types/debug": "4.1.9",
        "@types/fs-extra": "11.0.2",
        "@types/gunzip-maybe": "1.4.0",
        "@types/hosted-git-info": "3.0.3",
        "@types/jest": "^29.5.4",
        "@types/prettier": "2.7.3",
        "@types/setup-polly-jest": "0.5.3",
        "@types/tar-stream": "3.1.1",
        "clinic": "13.0.0",
        "dts-cli": "2.0.3",
        "eslint-plugin-prettier": "4.2.1",
        "fast-glob": "3.3.1",
        "fs-extra": "11.1.1",
        "husky": "8.0.3",
        "nock": "13.3.3",
        "np": "8.0.4",
        "setup-polly-jest": "0.11.0",
        "ts-dedent": "2.2.0",
        "tslib": "2.6.2",
        "tsx": "^3.12.8",
        "typescript": "5.2.2"
    }
}
